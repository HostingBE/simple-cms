          {% if paginator.numPages > 1 %}
            <!-- Pagination-->
            <nav class="border-top pb-md-4 pt-4 mt-2" aria-label="Pagination">
              <ul class="pagination mb-1">
                  {% if paginator.prevUrl %}
                <li class="page-item"><a class="page-link page-link-static" href="{{ paginator.prevUrl }}">&laquo; Previous</a></li>
              {% endif %}

              {% for page in paginator.pages %}
            {% if page.url %}
                     <li class="page-item d-none d-sm-block{{ page.isCurrent ? ' active' : '' }}"><a class="page-link" href="{{ page.url }}"">{{page.num}}</a></li>
              
            {% endif %}
        {% endfor %}


        {% if paginator.nextUrl %}
            <li class="page-item"><a class="page-link" href="{{ paginator.nextUrl }}">Next &raquo;</a></li>
        {% endif %}
              </ul>
            </nav>
            {% endif %}
